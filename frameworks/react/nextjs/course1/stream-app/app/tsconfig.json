{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["DOM", "DOM.Iterable","ESNext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx":"preserve",
      "baseUrl": "."
    },
    "include": ["next-env.d.ts","**/*.ts","**/*.tsx"],
    "exclude": ["node_modules"]
  }